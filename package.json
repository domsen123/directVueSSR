{
  "name": "directus-extension-vue-root-ssr-render",
  "version": "1.0.0",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-hook"
  ],
  "directus:extension": {
    "type": "hook",
    "path": "/absolute/path/to/your/directus/instance/hook/extension/folder/index.js",
    "source": "src/extension/index.ts",
    "host": "^9.10.0"
  },
  "scripts": {
    "clear": "rm -rf ./node_modules && pnpm i",
    "dev": "vite-ssr",
    "build": "npm-run-all --parallel build:extension build:app",
    "build:watch": "nodemon",
    "build:extension": "npx directus-extension build",
    "build:app": "vite-ssr build",
    "clean": "rm -rf ./dist",
    "clean:build": "pnpm clean && pnpm build"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "9.10.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/express": "^4.17.13",
    "@types/geojson": "^7946.0.8",
    "@types/node": "^17.0.33",
    "@types/pino": "^7.0.5",
    "@vitejs/plugin-vue": "^2.3.3",
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "knex": "^2.0.0",
    "knex-schema-inspector": "^1.7.5",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "pino": "^7.11.0",
    "rollup": "^2.72.1",
    "rollup-plugin-node-externals": "^4.0.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vue": "^3.2.33",
    "vue-router": "^4.0.15"
  },
  "dependencies": {
    "@nuxt/devalue": "^2.0.0",
    "@vueuse/core": "^8.4.2",
    "@vueuse/head": "^0.7.6",
    "vite-ssr": "^0.16.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/express",
        "@types/geojson",
        "@types/pino",
        "@types/node",
        "react",
        "react-dom",
        "geojson",
        "vue-i18n",
        "typescript"
      ]
    }
  }
}
